    "react-dom": "19.2.3",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "prettier": "^3.7.4",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "6.19.1",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
